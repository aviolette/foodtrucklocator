<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>chicagofoodtrucklocator</application>
  <version>3</version>
  <threadsafe>true</threadsafe>
  <static-files>
    <include path="/bootstrap2.2.2-custom/**" expiration="100d"/>
    <include path="/ace/**" expiration="100d"/>
    <include path="/css/**" expiration="100d"/>
    <include path="/img/**" expiration="100d"/>
    <include path="/script/**" expiration="100d"/>
  </static-files>
  <system-properties>
    <property name="use.local.frameworks" value="true"/>
    <property name="foodtrucklocator.title" value="Orlando Food Truck Finder"/>
    <property name="foodtrucklocator.showBoozy" value="true"/>
    <property name="foodtrucklocator.showStats" value="true"/>
    <property name="foodtrucklocator.signal.id" value="zIOrUTR"/>
    <property name="foodtrucklocator.state" value="IL"/>
    <property name="foodtrucklocator.city" value="Schaumburg"/>
    <property name="foodtrucklocator.baseUrl" value="http://localhost:8080"/>
  </system-properties>
</appengine-web-app>
